 <article>
  <p style="font-size:xx-large;text-align:center">Menus</p>

  <section class="Menu">
    <%@menus.each do |menu|%>
      <li>
        <%=link_to menu_path(menu.id),class: "Menu-link" do%>
          <label class="Menu-container">
            <p class="Menu-text"><%= menu.menu_name %></p>
          </label>
        <%end%>
      </li>
    <%end%>
  </section>
</article>